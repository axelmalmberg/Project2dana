<AnchorPane id="AnchorPane" prefHeight="525.0" prefWidth="750.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="project2dana.controller.ViewOrdersController">
   <children>
      <ImageView fitHeight="716.0" fitWidth="1003.0" pickOnBounds="true">
         <effect>
            <Glow level="0.56">
               <input>
                  <ColorAdjust brightness="0.02" contrast="0.12" />
               </input>
            </Glow>
         </effect>
         <image>
            <Image url="@background.jpg" />
         </image>
      </ImageView>
      <TableView fx:id="orderTable" layoutX="115.0" layoutY="172.0" opacity="0.8" prefHeight="353.0" prefWidth="774.0">
        <columns>
          <TableColumn fx:id="drinkColumn" prefWidth="75.0" text="Drink" />
          <TableColumn fx:id="drinkSizeColumn" prefWidth="78.0" text="Drink size" />
            <TableColumn fx:id="appetizerColumn" prefWidth="131.0" text="Appetizer" />
            <TableColumn fx:id="mainCourseColumn" prefWidth="117.0" text="Main course" />
            <TableColumn fx:id="dessertColumn" prefWidth="89.0" text="Dessert" />
            <TableColumn fx:id="extraColumn" prefWidth="83.0" text="Extra" />
            <TableColumn fx:id="tableColumn" prefWidth="48.0" text="Table" />
            <TableColumn fx:id="idColumn" prefWidth="31.0" text="Id" />
            <TableColumn fx:id="dateColumn" prefWidth="121.0" text="Date" />
        </columns>
      </TableView>
      <Rectangle arcHeight="5.0" arcWidth="5.0" fill="WHITE" height="54.0" layoutX="114.0" layoutY="547.0" opacity="0.8" stroke="WHITE" strokeType="INSIDE" strokeWidth="5.0" width="774.0" />
      <Button fx:id="returnButton" layoutX="794.0" layoutY="558.0" mnemonicParsing="false" onAction="#handleaddReturnButtonAction" text="Return">
         <font>
            <Font name="Times New Roman" size="18.0" />
         </font></Button>
      <Button fx:id="finishedOrderButton" layoutX="225.0" layoutY="558.0" mnemonicParsing="false" onAction="#handleFinishedButtonAction" text="Finished">
         <font>
            <Font name="Times New Roman" size="18.0" />
         </font></Button>
      <Button fx:id="deleteOrderButton" layoutX="329.0" layoutY="558.0" mnemonicParsing="false" onAction="#handleFinishedButtonAction" text="Delete">
         <font>
            <Font name="Times New Roman" size="18.0" />
         </font></Button>
      <Button fx:id="refreshButton" layoutX="135.0" layoutY="558.0" mnemonicParsing="false" onAction="#handleRefreshButtonAction" text="Refresh">
         <font>
            <Font name="Times New Roman" size="18.0" />
         </font></Button>
      <Rectangle arcHeight="5.0" arcWidth="5.0" fill="WHITE" height="54.0" layoutX="114.0" layoutY="94.0" opacity="0.8" stroke="WHITE" strokeType="INSIDE" strokeWidth="5.0" width="774.0" />
      <Label layoutX="466.0" layoutY="107.0" text="DANA">
         <font>
            <Font name="Times New Roman Bold" size="24.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
