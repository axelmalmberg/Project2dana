<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.shape.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="525.0" prefWidth="750.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="project2dana.ViewOrdersController">
   <children>
      <ImageView fitHeight="525.0" fitWidth="750.0" pickOnBounds="true">
         <effect>
            <Glow level="0.56">
               <input>
                  <ColorAdjust brightness="0.02" contrast="0.12" />
               </input>
            </Glow>
         </effect>
         <image>
            <Image url="@background.jpg" />
         </image>
      </ImageView>
      <TableView fx:id="orderTable" layoutX="26.0" layoutY="84.0" opacity="0.8" prefHeight="353.0" prefWidth="696.0">
        <columns>
          <TableColumn fx:id="drinkColumn" prefWidth="75.0" text="Drink" />
          <TableColumn fx:id="drinkSizeColumn" prefWidth="103.0" text="Drink size" />
            <TableColumn fx:id="appetizerColumn" prefWidth="99.0" text="Appetizer" />
            <TableColumn fx:id="mainCourseColumn" prefWidth="102.0" text="Main course" />
            <TableColumn fx:id="dessertColumn" text="Dessert" />
            <TableColumn fx:id="extraColumn" prefWidth="64.0" text="Extra" />
            <TableColumn fx:id="tableColumn" prefWidth="66.0" text="Table" />
            <TableColumn fx:id="idColumn" prefWidth="43.0" text="Id" />
            <TableColumn fx:id="dateColumn" prefWidth="63.0" text="Date" />
        </columns>
      </TableView>
      <Rectangle arcHeight="5.0" arcWidth="5.0" fill="WHITE" height="54.0" layoutX="26.0" layoutY="447.0" opacity="0.8" stroke="WHITE" strokeType="INSIDE" strokeWidth="5.0" width="696.0" />
      <Button fx:id="returnButton" layoutX="637.0" layoutY="458.0" mnemonicParsing="false" onAction="#handleaddReturnButtonAction" text="Return">
         <font>
            <Font name="Times New Roman" size="18.0" />
         </font></Button>
      <Button fx:id="finishedOrderButton" layoutX="132.0" layoutY="458.0" mnemonicParsing="false" text="Finished">
         <font>
            <Font name="Times New Roman" size="18.0" />
         </font></Button>
      <Button fx:id="deleteOrderButton" layoutX="235.0" layoutY="458.0" mnemonicParsing="false" text="Delete">
         <font>
            <Font name="Times New Roman" size="18.0" />
         </font></Button>
      <Button fx:id="refreshButton" layoutX="39.0" layoutY="458.0" mnemonicParsing="false" text="Refresh">
         <font>
            <Font name="Times New Roman" size="18.0" />
         </font></Button>
      <Rectangle arcHeight="5.0" arcWidth="5.0" fill="WHITE" height="54.0" layoutX="26.0" layoutY="22.0" opacity="0.8" stroke="WHITE" strokeType="INSIDE" strokeWidth="5.0" width="696.0" />
      <Label layoutX="339.0" layoutY="35.0" text="DANA">
         <font>
            <Font name="Times New Roman Bold" size="24.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
